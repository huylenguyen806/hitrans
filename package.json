{
  "name": "hitrans",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.0.0-rc.0",
    "@material-ui/icons": "^4.1.0",
    "electron": "^5.0.3",
    "electron-is-dev": "^1.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-transition-group": "^4.1.1",
    "x11": "^2.3.0",
    "zerorpc": "^0.9.8"
  },
  "description": "Highlight Translation",
  "main": "main.js",
  "scripts": {
    "start": "npm run electron",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "concurrently --kill-others 'npm run start-react' 'npm run start-electron'",
    "start-react": "BROWSER=none react-scripts start",
    "start-electron": "wait-on http-get://localhost:3000 && electron . --debug",
    "electron-version": "electron --version"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron-builder": "^20.43.0",
    "electron-rebuild": "^1.8.5",
    "wait-on": "^3.2.0"
  }
}
